<html lang="FR-fr">
<head>
    <meta charset='UTF-8'>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta http-equiv="X-UA-Compatible" content="ie=edge"/>
    <title>Lajavel</title>
    <meta name="description" content="Lajavel is a new Java Web Framework made for hardcore developers."/>
    <meta name="keywords" content="lajavel,laravel,framework"/>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/themes/prism-okaidia.min.css">
    <link rel="stylesheet"
          href="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/plugins/unescaped-markup/prism-unescaped-markup.min.css">
    <link rel="stylesheet" href="{{ assets(css) }}">
    <script src="{{ assets(js) }}" async></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/prism.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/plugins/autoloader/prism-autoloader.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.29.0/plugins/unescaped-markup/prism-unescaped-markup.min.js"></script>
    {% include tailwind %}
</head>

<body>
<!-- Navigation -->
{% include header %}

<!-- Get started -->
<section
        class="pt-32 pb-10 md:mt-0 md:h-auto flex flex-col justify-center text-center md:text-left md:flex-row md:justify-between md:items-center lg:px-48 md:px-12 px-4 bg-secondary">
    <div class="md:flex-1 md:mr-10">

        <h1 class="text-5xl font-montserrat font-bold">Get Started</h1>

        {% include start %}

        {% include register %}

        {% include make %}
    </div>
</section>

<!-- Footer -->
{% include footer %}
</body>
</html>